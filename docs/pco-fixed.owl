<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY schema "http://schema.org/" >
    <!ENTITY c4n "http://vocab.deri.ie/c4n#" >
    <!ENTITY dcterms "http://purl.org/dc/terms/" >
    <!ENTITY loted "http://loted.eu/ontology#" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY vann "http://purl.org/vocab/vann/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY vcard "http://www.w3.org/2006/vcard/ns#" >
    <!ENTITY gr "http://purl.org/goodrelations/v1#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY qb "http://purl.org/linked-data/cube#" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY payment "http://reference.data.gov.uk/def/payment#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY pc "http://purl.org/procurement/public-contracts#" >
    <!ENTITY pcdt "http://purl.org/procurement/public-contracts-datatypes#" >
]>


<rdf:RDF xmlns="http://www.unizar.es/ontologies/public-contracts#"
     xml:base="http://www.unizar.es/ontologies/public-contracts"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:payment="http://reference.data.gov.uk/def/payment#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:qb="http://purl.org/linked-data/cube#"
     xmlns:vcard="http://www.w3.org/2006/vcard/ns#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:schema="http://schema.org/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:loted="http://loted.eu/ontology#"
     xmlns:pc="http://purl.org/procurement/public-contracts#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:c4n="http://vocab.deri.ie/c4n#"
     xmlns:pcdt="http://purl.org/procurement/public-contracts-datatypes#"
     xmlns:gr="http://purl.org/goodrelations/v1#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#">
    <owl:Ontology rdf:about="http://purl.org/procurement/public-contracts#">
        <dcterms:modified rdf:datatype="&xsd;date">2011-11-18</dcterms:modified>
        <vann:preferredNamespaceUri>http://purl.org/procurement/public-contracts#</vann:preferredNamespaceUri>
        <vann:preferredNamespacePrefix>pc</vann:preferredNamespacePrefix>
        <dcterms:title xml:lang="cs">Ontologie pro veřejné zakázky</dcterms:title>
        <dcterms:description xml:lang="cs">Ontologie pro veřejné zakázky poskytuje vyjadřovací prostředky pro popis zakázek zadávaných institucemi veřejné správy.</dcterms:description>
        <dcterms:title xml:lang="en">Public Contracts Ontology</dcterms:title>
        <dcterms:description xml:lang="en">Public Contracts Ontology offers a means of expression for describing public contracts in the public sector.</dcterms:description>
        <rdfs:seeAlso rdf:resource="http://code.google.com/p/public-contracts-ontology/"/>
        <dcterms:rights rdf:resource="http://creativecommons.org/licenses/by/3.0/cz/"/>
        <dcterms:creator rdf:resource="http://keg.vse.cz/resource/person/jindrich-mynarz"/>
        <dcterms:creator rdf:resource="http://keg.vse.cz/resource/person/josef-petrak"/>
        <dcterms:creator rdf:resource="http://keg.vse.cz/resource/person/vojtech-svatek"/>
        <rdfs:seeAlso rdf:resource="http://prefix.cc/pc"/>
        <dcterms:creator rdf:resource="http://purl.org/klimek#me"/>
        <dcterms:creator rdf:resource="http://www.facebook.com/nohis"/>
        <dcterms:creator rdf:resource="http://www.ksi.mff.cuni.cz/~necasky"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/procurement/public-contracts-datatypes#percentage -->

    <rdfs:Datatype rdf:about="&pcdt;percentage">
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="&xsd;integer">0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="&xsd;integer">100</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </owl:equivalentClass>
    </rdfs:Datatype>
    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="&xsd;date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/subject -->

    <owl:ObjectProperty rdf:about="&dcterms;subject"/>
    


    <!-- http://purl.org/goodrelations/v1#hasPriceSpecification -->

    <owl:ObjectProperty rdf:about="&gr;hasPriceSpecification"/>
    


    <!-- http://purl.org/procurement/public-contracts#actualPrice -->

    <owl:ObjectProperty rdf:about="&pc;actualPrice">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="cs">Skutečná cena po realizaci zakázky</rdfs:label>
        <rdfs:label xml:lang="en">Actual price after contract realization</rdfs:label>
        <rdfs:comment xml:lang="cs">Vlastnost pro skutečnou cenu zaplacenou za zakázku. Kardinalita 0..1</rdfs:comment>
        <rdfs:comment xml:lang="en">Property for actual price of the public contract. Cardinality 0..1</rdfs:comment>
        <rdfs:range rdf:resource="&gr;PriceSpecification"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:subPropertyOf rdf:resource="&pc;contractPrice"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#additionalObject -->

    <owl:ObjectProperty rdf:about="&pc;additionalObject">
        <rdfs:label xml:lang="cs">Další předmět zakázky</rdfs:label>
        <rdfs:label xml:lang="en">Additional object of contract</rdfs:label>
        <rdfs:comment xml:lang="cs">Vlastnost pro další předmět veřejné zakázky. Kardinalita 0..*</rdfs:comment>
        <rdfs:comment xml:lang="en">Property for additional object of public contract. Cardinality 0..*</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dcterms;subject"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:domain rdf:resource="&pc;Contract"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#agreedPrice -->

    <owl:ObjectProperty rdf:about="&pc;agreedPrice">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="cs">Smluvní cena</rdfs:label>
        <rdfs:label xml:lang="en">Agreed price</rdfs:label>
        <rdfs:comment xml:lang="cs">Vlastnost pro cenu napsanou na smlouvě s dodavatelem. Kardinalita 0..1</rdfs:comment>
        <rdfs:comment xml:lang="en">Property for a price stated on the contract with the supplier. Cardinality 0..1</rdfs:comment>
        <rdfs:range rdf:resource="&gr;PriceSpecification"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:subPropertyOf rdf:resource="&pc;contractPrice"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#agreement -->

    <owl:ObjectProperty rdf:about="&pc;agreement">
        <rdfs:label xml:lang="cs">Smlouva na veřejnou zakázku</rdfs:label>
        <rdfs:label xml:lang="en">Contract agreement</rdfs:label>
        <rdfs:comment xml:lang="cs">Vlastnost pro připojení dokumentů obsahujících smlouvy na veřejnou zakázku uzavřené mezi zadavatelem a dodavatelem. Kardinalita 0..*</rdfs:comment>
        <rdfs:comment xml:lang="en">Property for attachment of documents containing the agreement between supplier and contractor. Cardinality 0..*</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:subPropertyOf rdf:resource="&pc;attachment"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#attachment -->

    <owl:ObjectProperty rdf:about="&pc;attachment">
        <rdfs:label xml:lang="cs">Příloha k zakázce</rdfs:label>
        <rdfs:label xml:lang="en">Contract attachment</rdfs:label>
        <rdfs:comment xml:lang="cs">Vlastnost pro připojení ostatních příhloh. Specifikaci připojte pomocí pc:specification, smlouvu pomocí pc:agreement. Kardinalita 0..*</rdfs:comment>
        <rdfs:comment xml:lang="en">Property for attachment of other documents. For attachment of contract specification use pc:specification. For attachment of contract agreement use pc:agreement. Cardinality 0..*</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:domain rdf:resource="&pc;Contract"/>
        <rdfs:range rdf:resource="&rdf;Resource"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#awardCriteriaCombination -->

    <owl:ObjectProperty rdf:about="&pc;awardCriteriaCombination">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="cs">Kombinace kritérií pro hodnocení nabídek dodavatelů zakázky.</rdfs:label>
        <rdfs:label xml:lang="en">Criteria combination for tender evaluation.</rdfs:label>
        <rdfs:comment xml:lang="cs">Vlastnost pro nastavení kombinace kritérií pro hodnocení nabídek dodavatelů. Kardinalita 0..1</rdfs:comment>
        <rdfs:comment xml:lang="en">Property for criteria combination for tender evaluation. Cardinality 0..1</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:range rdf:resource="&pc;AwardCriteriaCombination"/>
        <rdfs:domain rdf:resource="&pc;Contract"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#awardCriterion -->

    <owl:ObjectProperty rdf:about="&pc;awardCriterion">
        <rdfs:label xml:lang="cs">Kritérium pro zadání zakázky.</rdfs:label>
        <rdfs:label xml:lang="de">Vergabekriterien</rdfs:label>
        <rdfs:label xml:lang="en">Critera for contract award.</rdfs:label>
        <rdfs:label xml:lang="fr">Critères d&apos;attribution</rdfs:label>
        <rdfs:comment xml:lang="cs">Vlastnost pro nastavení jednoho kritéria pro hodnocení nabídek dodavatelů. Kardinalita 0..*</rdfs:comment>
        <rdfs:comment xml:lang="en">Property for setting a criterion for tender evaluation. Cardinality 0..*</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:domain rdf:resource="&pc;AwardCriteriaCombination"/>
        <rdfs:range rdf:resource="&pc;AwardCriterion"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#awardedTender -->

    <owl:ObjectProperty rdf:about="&pc;awardedTender">
        <rdfs:label xml:lang="cs">Vybraná nabídka</rdfs:label>
        <rdfs:label xml:lang="en">Awarded tender</rdfs:label>
        <rdfs:comment xml:lang="cs">Vlastnost pro vítěznou nabídku dodavatele. Kardinalita 0..1</rdfs:comment>
        <rdfs:comment xml:lang="en">Property for awarded tender submitted by a supplier. Cardinality 0..1</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:subPropertyOf rdf:resource="&pc;tender"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#buyerProfile -->

    <owl:ObjectProperty rdf:about="&pc;buyerProfile">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label xml:lang="cs">Profilová stránka kupujícího</rdfs:label>
        <rdfs:label xml:lang="en">Buyers&apos;s profile website</rdfs:label>
        <rdfs:comment xml:lang="cs">Vlastnost pro profilovou stránku kupujícího. Kardinalita 0..1</rdfs:comment>
        <rdfs:comment xml:lang="en">Property for buyer&apos;s profile. Cardinality 0..1</rdfs:comment>
        <rdfs:domain rdf:resource="&gr;BusinessEntity"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:range rdf:resource="&rdf;Resource"/>
        <rdfs:subPropertyOf rdf:resource="&foaf;homepage"/>
        <rdfs:subPropertyOf rdf:resource="&foaf;page"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#contact -->

    <owl:ObjectProperty rdf:about="&pc;contact">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="cs">Hlavní kontakt pro zakázku</rdfs:label>
        <rdfs:label xml:lang="en">Main contact for a contract</rdfs:label>
        <rdfs:comment xml:lang="cs">Hlavní kontakt pro zakázku. Kardinalita 0..1</rdfs:comment>
        <rdfs:comment xml:lang="en">Main contact for a contract. Cardinality 0..1</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:domain rdf:resource="&pc;Contract"/>
        <rdfs:range rdf:resource="&vcard;VCard"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#contractPrice -->

    <owl:ObjectProperty rdf:about="&pc;contractPrice">
        <rdfs:label xml:lang="cs">Cena</rdfs:label>
        <rdfs:label xml:lang="en">Price</rdfs:label>
        <rdfs:comment xml:lang="cs">Abstraktní vlastnost pro cenu veřejné zakázky</rdfs:comment>
        <rdfs:comment xml:lang="en">Abstract property for price of public contract</rdfs:comment>
        <rdfs:range rdf:resource="&gr;PriceSpecification"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:domain rdf:resource="&pc;Contract"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#contractingAuthority -->

    <owl:ObjectProperty rdf:about="&pc;contractingAuthority">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="cs">Zadavatel</rdfs:label>
        <rdfs:label xml:lang="en">Contracting authority</rdfs:label>
        <rdfs:comment xml:lang="cs">Zadavatel zakázky. Kardinalita 0..1</rdfs:comment>
        <rdfs:comment xml:lang="en">Contracting authority. Cardinality 0..1</rdfs:comment>
        <rdfs:range rdf:resource="&gr;BusinessEntity"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:domain rdf:resource="&pc;Contract"/>
        <rdfs:subPropertyOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="&c4n;issues"/>
            </rdf:Description>
        </rdfs:subPropertyOf>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#estimatedPrice -->

    <owl:ObjectProperty rdf:about="&pc;estimatedPrice">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="cs">Přesná odhadovaná cena cena</rdfs:label>
        <rdfs:label xml:lang="en">Exact estimated price</rdfs:label>
        <rdfs:comment xml:lang="cs">Vlastnost pro přesnou odhadovanou cenu veřejné zakázky. Pokud je odhadovanou cenou interval, použijte vlastnosti pc:estimatedPriceLower a pc:estimatedPriceUpper. Kardinalita 0..1</rdfs:comment>
        <rdfs:comment xml:lang="en">Property for exact estimated price of public contract. If only estimated interval is known, use pc:estimatedPriceLower and pc:estimatedPriceUpper properties instead. Cardinality 0..1</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:subPropertyOf rdf:resource="&pc;contractPrice"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#estimatedPriceLower -->

    <owl:ObjectProperty rdf:about="&pc;estimatedPriceLower">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="cs">Dolní mez odhadované ceny</rdfs:label>
        <rdfs:label xml:lang="en">Lower range of estimated price</rdfs:label>
        <rdfs:comment xml:lang="cs">Vlastnost pro dolní mez odhadované ceny veřejné zakázky. Pokud je známa přesná odhadovaná cena, použijte vlastnost pc:estimatedPrice. Kardinalita 0..1</rdfs:comment>
        <rdfs:comment xml:lang="en">Property for lower bound of estimated price of public contract. If exact estimated price is known, use pc:estimatedPrice instead. Cardinality 0..1</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:subPropertyOf rdf:resource="&pc;contractPrice"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#estimatedPriceUpper -->

    <owl:ObjectProperty rdf:about="&pc;estimatedPriceUpper">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="cs">Horní mez odhadované ceny</rdfs:label>
        <rdfs:label xml:lang="en">Upper range of estimated price</rdfs:label>
        <rdfs:comment xml:lang="cs">Vlastnost pro horní mez odhadované ceny veřejné zakázky. Pokud je známa přesná odhadovaná cena, použijte vlastnost pc:estimatedPrice. Kardinalita 0..1</rdfs:comment>
        <rdfs:comment xml:lang="en">Property for upper bound of estimated price of public contract. If exact estimated price is known, use pc:estimatedPrice instead. Cardinality 0..1</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:subPropertyOf rdf:resource="&pc;contractPrice"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#item -->

    <owl:ObjectProperty rdf:about="&pc;item">
        <rdfs:comment xml:lang="cs">Vlastnost pro přiřazení položky veřejné zakázce. Kardinalita 0..*</rdfs:comment>
        <rdfs:comment xml:lang="en">Property for attachment of an Item to a public contract. Cardinality 0..*</rdfs:comment>
        <rdfs:range rdf:resource="&gr;Offering"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&loted;Tender"/>
                    <rdf:Description rdf:about="&pc;Contract"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#kind -->

    <owl:ObjectProperty rdf:about="&pc;kind">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="cs">Druh zakázky</rdfs:label>
        <rdfs:label xml:lang="en">Type of contract</rdfs:label>
        <rdfs:comment xml:lang="cs">Druh zakázky. Kardinalita 0..1</rdfs:comment>
        <rdfs:comment xml:lang="en">Type or kind of contract. Cardinality 0..1</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:domain rdf:resource="&pc;Contract"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&skos;inScheme"/>
                <owl:hasValue rdf:resource="&pc;KindScheme"/>
            </owl:Restriction>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#location -->

    <owl:ObjectProperty rdf:about="&pc;location">
        <rdfs:label xml:lang="cs">Místo uskutečnění zakázky</rdfs:label>
        <rdfs:label xml:lang="en">Place of contract&apos;s realization</rdfs:label>
        <rdfs:comment xml:lang="cs">Místo, kde dojde k dodání produktu nebo služby nasmlouvané ve veřejné zakázce. Kardinalita 0..*</rdfs:comment>
        <rdfs:comment xml:lang="en">Place of delivery of contracted product or service. Cardinality 0..*</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:domain rdf:resource="&pc;Contract"/>
        <rdfs:range rdf:resource="&schema;Place"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#lot -->

    <owl:ObjectProperty rdf:about="&pc;lot">
        <rdfs:label xml:lang="cs">Část zakázky</rdfs:label>
        <rdfs:label xml:lang="en">Contract lot</rdfs:label>
        <rdfs:comment xml:lang="cs">Vlastnost pro specifikaci části zakázky. Zakázku rozděluje na části zadavatel, pokud je zakázka přiliš velká. Dodavatelé podávají nabídky na každou část odděleně a pro každou část zakázky je vybrán samostatný dodavatel. Pro samotnou zakázku nabídky nejsou podávány. Kardinalita 0..*</rdfs:comment>
        <rdfs:comment xml:lang="en">Property for specifying contract lot. When contract is too big, it is split to lots by contracting authority. Suppliers then tender each lot separately. For each lot the authority awards a separate tender. The contract itself has no tenders. Cardinality 0..*</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:range rdf:resource="&pc;Contract"/>
        <rdfs:domain rdf:resource="&pc;Contract"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#mainObject -->

    <owl:ObjectProperty rdf:about="&pc;mainObject">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="cs">Hlavní předmět zakázky</rdfs:label>
        <rdfs:label xml:lang="en">Main object of contract</rdfs:label>
        <rdfs:comment xml:lang="cs">Vlastnost pro hlavní předmět veřejné zakázky. Kardinalita 0..1</rdfs:comment>
        <rdfs:comment xml:lang="en">Property for main object of public contract. Cardinality 0..1</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/procurement/public-contracts#"/>
        <rdfs:domain rdf:resource="&pc;Contract"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/procurement/public-contracts#offeredPrice -->

    <owl:ObjectProperty rdf:about="&pc;offeredPrice">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="cs">Nabídnutá cena v nabídce</rdfs:label>
        <rdfs:label xml:lang="en">Offered price in tender</rdfs:label>
        <rdfs:comment xml:lang="cs">Vlastnost pro cenu nabídnutou dodavatelem. Kardinalita 0..1</rdfs:comment>
